<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout"
        android:visibility="visible"
        android:theme="@style/AppTheme"
        >

    <ImageView
            android:id="@+id/table_imageview"
            android:layout_width="200dp"
            android:layout_height="185dp"
            app:srcCompat="@mipmap/table_free"
            tools:layout_conversion_absoluteHeight="129dp"
            tools:layout_editor_absoluteY="8dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_conversion_absoluteWidth="145dp" />

    <TextView
            android:id="@+id/table_room_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="dsadad"
            android:textSize="14sp"
            android:textStyle="bold"
            tools:layout_conversion_absoluteHeight="19dp"
            tools:layout_editor_absoluteY="145dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_conversion_absoluteWidth="161dp"
            android:textAlignment="center" />

    <TextView
            android:id="@+id/table_state_textview"
            android:text="dsada"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            tools:layout_conversion_absoluteHeight="19dp"
            tools:layout_editor_absoluteY="164dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_conversion_absoluteWidth="161dp" />


</LinearLayout>







